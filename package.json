  },
  "homepage": "https://github.com/inofix/note-it-js#readme",
  "devDependencies": {
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-static-files-copy": "^1.2.4",
    "serve": "^10.0.2"
  },
  "dependencies": {
    "d3": "5.7.0"
